{
  "name": "@web-builder/frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development npm-run-all clean webpack:dev:*",
    "webpack:dev:client": "webpack --config=webpack.config.ts --env mode=development --env type=web",
    "webpack:dev:server": "webpack --config=webpack.config.ts --env mode=development --env type=web --env preset=server",
    "build": "NODE_ENV=production npm-run-all clean webpack:production:*",
    "webpack:production:client": "webpack --config=webpack.config.ts --env mode=production --env type=web",
    "webpack:production:server": "webpack --config=webpack.config.ts --env mode=production --env type=web --env preset=server",
    "clean": "rm -rf ./dist",
    "lint": "npm run --prefix ../../ lint",
    "lint:fix": "npm run --prefix ../../ lint:fix",
    "dev": "DEBUG=server:*,express:* npm-run-all clean serve:dev",
    "serve:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register devServer.ts",
    "start": "DEBUG=server:* node dist/server.js",
    "typecheck": "tsc --noEmit"
  },
  "author": "Carlos Santana",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@contentpi/ui-kit": "^2.0.9",
    "@web-builder/design-system": "1.0.0",
    "@web-builder/devtools": "1.0.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "serialize-javascript": "^6.0.0",
    "webpack": "^5.70.0",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/serialize-javascript": "^5.0.2",
    "@types/styled-components": "^5.1.24",
    "@types/webpack-env": "^1.16.3",
    "ts-jest": "^27.1.3"
  }
}
