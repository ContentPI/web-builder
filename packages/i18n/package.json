{
  "name": "@web-builder/i18n",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production npm-run-all clean compile webpack:production",
    "build:dev": "NODE_ENV=development npm-run-all clean compile webpack:development",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "lint": "npm run --prefix ../../ lint",
    "lint:fix": "npm run --prefix ../../ lint:fix",
    "prepublishOnly": "npm run lint && npm run build",
    "webpack:development": "npm run --prefix ../../ i18n:webpack:development",
    "webpack:production": "npm run --prefix ../../ i18n:webpack:production"
  },
  "author": "Carlos Santana",
  "license": "MIT",
  "dependencies": {
    "@web-builder/devtools": "^1.0.0",
    "react": "^17.0.2",
    "webpack": "^5.70.0",
    "webpack-merge": "^5.8.0"
  }
}
